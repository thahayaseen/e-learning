{
  "compilerOptions": {
    "target": "ESNext",  
    "module": "CommonJS",  
 
    "strict": true,  
    "moduleResolution": "Node",  
    "allowJs": true,  
    "outDir": "./dist",  
    "rootDir": "./src",  
    "esModuleInterop": true,  
    "resolveJsonModule": true,  
    "skipLibCheck": true,  
    "forceConsistentCasingInFileNames": true  
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
